# Logger
LOG_LEVEL=debug
LOG_TIME_FORMAT=YYYY-MM-DDTHH:mm:ss.SSS
LOG_TO_FILE=true
LOG_FILE_PATH=
## log file size in mega
LOG_FILE_MAXSIZE=10
