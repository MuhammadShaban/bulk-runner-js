# Global environment variables
NODE_ENV=develop
DEBUG=true

PORT=4000
UPLOAD_PATH=

# Results
RESULTS_DRIVER=local
RESULTS_TEMP_PATH=
RESULTS_SAVE_PATH=

# Runner
RUNNER_THROUGHPUT=100
REQUEST_RETRIES_COUNT=5
REQUEST_RETRIES_DELAY=1000
